{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "format": "prettier --check .",
    "format:fix": "prettier --write --list-different src/**/*.{ts,tsx}",
    "check-types": "yarn tsc --noEmit --incremental false",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --watchAll=false --maxWorkers=50%",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "docker:build": "docker build -t soterrenos-front .",
    "docker:run": "docker run -e PORT=8080 -p 8080:8080 soterrenos-front",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.0",
    "@radix-ui/react-accordion": "^1.2.3",
