    "prettier": "^3.5.0",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^4.0.6",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "yarn": "^1.22.22"
  }
